{
  "name": "@bhoos/auth-server",
  "version": "1.0.0",
  "description": "Bhoos Authentication Library for Server",
  "main": "dist/index.js",
  "module": "es6/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./es6/index.js"
  },
  "files": [
    "dist",
    "es6",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es6",
    "build:cjs": "tsc",
    "build:es6": "tsc --module es2020 --outDir es6 && echo '{\"type\":\"module\"}' > es6/package.json",
    "build": "yarn clean && yarn build:es6 && yarn build:cjs",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@bhoos/auth-common": "^1.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.11",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "git@github.com:bhoos/auth",
  "author": "Ranjan Shrestha",
  "license": "UNLICENSED"
}
