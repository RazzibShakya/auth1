{
  "name": "@bhoos/auth-common",
  "version": "2.0.4",
  "description": "Bhoos Authentication Common",
  "main": "dist/index.js",
  "module": "es6/index.js",
  "react-native": "src/index.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./es6/index.js"
  },
  "files": [
    "dist",
    "es6",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es6",
    "build:cjs": "tsc",
    "build:es6": "tsc --module es2020 --outDir es6 && echo '{\"type\":\"module\"}' > es6/package.json",
    "build": "yarn clean && yarn build:es6 && yarn build:cjs",
    "prepublishOnly": "yarn build",
    "release": "npm dist-tag add ${npm_package_name}@${npm_package_version} latest"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "git@github.com:bhoos/auth",
  "author": "Ranjan Shrestha",
  "license": "UNLICENSED",
  "private": false,
  "gitHead": "ad107830a70f598b380e8cc8fbe6ef4384d9f635"
}
